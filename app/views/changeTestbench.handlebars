<nav class="navbar navbar-default">
    <h1>Change testbench for {{assignment.name}} in {{course.code}}</h1>
</nav>
<div class="row row-cols-1 row-cols-md-2 g-4 width: 18rem">
    {{#if assignment.testbenchIsNull}}
    <p class="text-danger">No testbench could be found for {{assignment.name}}</p>
    {{/if}}
    <form method="POST" autocomplete="off" enctype="multipart/form-data">
        <div class="form-group">
            <label for="fileUpload">Select a file:</label>
            <input type="file" id="fileUpload" name="fileUpload">
        </div>

        <button type="submit" class="btn btn-primary mt-3" href="#">Submit</button>
    </form>
</div>